{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    },
    {
      "src": "api/*.js",
      "use": "@vercel/edge"
    }
  ],
  "routes": [
    {
      "src": "/api/generate-document",
      "dest": "/api/generate-document.js"
    },
    {
      "src": "/api/efile-status",
      "dest": "/api/efile-status.js"
    },
    {
      "src": "/(.*)",
      "dest": "/build/$1"
    }
  ]
}
